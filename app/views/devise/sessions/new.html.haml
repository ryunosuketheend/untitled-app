.form
  = form_with model: @user, url: user_session_path, id: 'new_user', class: 'form__contents', local: true do |f|
    %h3 Untitled
    %h2 Log in
    .field
      = f.label :email
      %br/
      = f.email_field :email, autofocus: true, autocomplete: "email"
    .field
      = f.label :password
      %br/
      = f.password_field :password, autocomplete: "current-password"
    -# - if devise_mapping.rememberable?
    -#   .field
    -#     = f.check_box :remember_me
    -#     = f.label :remember_me
    .action
      = f.submit "Log in"
    .action
      = render "devise/shared/links"
